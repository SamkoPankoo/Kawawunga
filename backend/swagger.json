    "/history/admin/all": {
      "get": {
        "tags": ["history"],
        "summary": "Get all user activity (admin only)",
        "description": "Get activity history for all users (requires admin privileges)",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "schema": {
              "type": "integer",
              "default": 1
            },
            "description": "Page number for pagination"
          },
          {
            "name": "limit",
            "in": "query",
            "schema": {
              "type": "integer",
              "default": 20
            },
            "description": "Number of records per page"
          }
        ],
        "responses": {
          "200": {
            "description": "Activity history retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer",
                            "example": 123
                          },
                          "userId": {
                            "type": "integer",
                            "example": 456
                          },
                          "action": {
                            "type": "string",
                            "example": "pdf-merge"
                          },
                          "description": {
                            "type": "string",
                            "example": "Merged 3 PDF files"
                          },
                          "ipAddress": {
                            "type": "string",
                            "example": "192.168.1.1"
                          },
                          "city": {
                            "type": "string",
                            "example": "Bratislava"
                          },
                          "country": {
                            "type": "string",
                            "example": "Slovakia"
                          },
                          "accessType": {
                            "type": "string",
                            "enum": ["frontend", "api"],
                            "example": "frontend"
                          },
                          "createdAt": {
                            "type": "string",
                            "format": "date-time",
                            "example": "2025-05-10T15:30:00.000Z"
                          },
                          "User": {
                            "type": "object",
                            "properties": {
                              "email": {
                                "type": "string",
                                "example": "user@example.com"
                              }
                            }
                          }
                        }
                      }
                    },
                    "pagination": {
                      "type": "object",
                      "properties": {
                        "total": {
                          "type": "integer",
                          "example": 100
                        },
                        "page": {
                          "type": "integer",
                          "example": 1
                        },
                        "pages": {
                          "type": "integer",
                          "example": 5
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Authentication required",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
