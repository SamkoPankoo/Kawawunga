    "/pdfLogs/log": {
      "post": {
        "tags": ["pdf-logs"],
        "summary": "Log PDF operation",
        "description": "Record a PDF operation in the logs",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "action": {
                    "type": "string",
                    "description": "The action performed (without pdf- prefix)",
                    "example": "merge"
                  },
                  "description": {
